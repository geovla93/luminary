{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@ui/core/*": ["./src/ui/core/*"],
      "@ui/core": ["./src/ui/core"],
      "@redux/*": ["./src/redux/*"],
      "@redux": ["./src/redux"],
      "@assets/*": ["./src/assets/*"],
      "@assets": ["./src/assets"],
      "@components/*": ["./src/components/*"],
      "@components": ["./src/components"],
      "@screens/*": ["./src/screens/*"],
      "@screens": ["./src/screens"],
      "@utils/*": ["./src/utils/*"],
      "@utils": ["./src/utils"],
      "@hooks/*": ["./src/hooks/*"],
      "@hooks": ["./src/hooks"],
      "i18n/*": ["./src/i18n/*"],
      "i18n": ["./src/i18n"],
      "@itypes/*": ["./src/types/*"],
      "@itypes": ["./src/types"]
    }
  }
}
